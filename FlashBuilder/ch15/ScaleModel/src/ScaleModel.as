package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;			public class ScaleModel extends Sprite{		public function ScaleModel():void{			init();		}		private function init():void{			var height:Number = 200;			var radius:Number = 4;						with (this.graphics){				lineStyle(1);				moveTo(0,300);				lineTo(550,300);				beginFill(0x000000,1);				drawRect(50,100,200,height);				endFill();			}						var ball:Ball;			ball = new Ball(radius,0x000000,1);			ball.pos2D = new Vector2D(300,height-radius-100);			addChild(ball);				var mover:ScaleModelMover=new ScaleModelMover(ball);			mover.startTime();				}	}}