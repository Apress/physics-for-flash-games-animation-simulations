package com.physicscodes.math {		import flash.geom.Vector3D;		public class Vector3DX extends Vector3D {			public function Vector3DX(px:Number=0,py:Number=0,pz:Number=0,pw:Number=0):void {			super(px,py,pz,pw);		}		public function unit():Vector3DX {			if (length > 0) {				return new Vector3DX(x/length,y/length,z/length,w);			}else{				return new Vector3DX(0,0,0,0);			}		}				public function multiply(k:Number):Vector3DX {			return new Vector3DX(x*k,y*k,z*k,w);		}				public function addScaled(vec:Vector3DX,k:Number):Vector3DX {			return new Vector3DX(x + k*vec.x,y + k*vec.y,z + k*vec.z,w);		}			static public function convert(vec:Vector3D):Vector3DX{			return new Vector3DX(vec.x,vec.y,vec.z,vec.w);		}	}}