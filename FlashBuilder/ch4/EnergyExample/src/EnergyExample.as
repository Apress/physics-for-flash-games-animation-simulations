package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;			public class EnergyExample extends Sprite{		public function EnergyExample():void{			init();		}		private function init():void{			var ball:Ball;			ball = new Ball(15,0x000000,1);			ball.pos2D = new Vector2D(50,50);			ball.velo2D = new Vector2D(20,0);			addChild(ball);				var accelerator:Accelerator=new Accelerator(ball);			accelerator.startTime();				}	}}