package{	import flash.display.Sprite;	import com.physicscodes.math.Vector2D;		import com.physicscodes.objects.Ball;	public class LorentzForce extends Sprite{		public function LorentzForce():void{			init();		}		private function init():void{			var particle:Ball;			particle = new Ball(5,0xff0000,1,1);				particle.pos2D = new Vector2D(400,500);			particle.velo2D = new Vector2D(40,0);			addChild(particle);			var timeStep:Number = 10;			var lorentz:Lorentz=new Lorentz(particle);					lorentz.startTime(timeStep);		}	}}