package{	import flash.display.Sprite;	import flash.filters.GlowFilter;			public class Fireworks2 extends Sprite{		public function Fireworks2():void{			init();		}		private function init():void{  			var sprite:Sprite = new Sprite();			addChild(sprite);			var mover:FireworksMover2=new FireworksMover2(sprite);					mover.startTime();						filters = [new GlowFilter()];			}	}}