package{	import flash.display.Sprite;	import com.physicscodes.math.Vector2D;		import com.physicscodes.objects.TurbineRB;	public class WindTurbines extends Sprite{		public function WindTurbines():void{			init();		}		private function init():void{			var turbines:Array = new Array();			var cm:Vector2D;						cm = new Vector2D(200,150);			var turbine1:TurbineRB = new TurbineRB(4,50,cm);						turbine1.mass = 1;			turbine1.momentOfInertia = 1;			addChild(turbine1);					turbines.push(turbine1);						cm = new Vector2D(150,400);			var turbine2:TurbineRB = new TurbineRB(6,75,cm);				turbine2.mass = 2.25;			turbine2.momentOfInertia = 5;						addChild(turbine2);					turbines.push(turbine2);									cm = new Vector2D(500,300);			var turbine3:TurbineRB = new TurbineRB(12,150,cm);				turbine3.mass = 9;			turbine3.momentOfInertia = 81;						addChild(turbine3);					turbines.push(turbine3);						var mover:TurbinesMover = new TurbinesMover(turbines);			mover.startTime();					}	}}