package{	import flash.display.Sprite;		import flash.geom.Vector3D;	public class VectorExamples extends Sprite{		private var vec1:Vector3D;		private var vec2:Vector3D;		public function VectorExamples(){			init();		}		private function init():void{			Vector2DExamples()			Vector3DExamples();		}				private function Vector2DExamples():void{			var vec1:Vector2D = new Vector2D(1,1);			var vec2:Vector2D = new Vector2D(1,0);						trace(vec1.length); // magnitude of vec1 			trace(vec1.lengthSquared); // magnitude squared of vec1						trace(Vector2D.angleBetween(vec1,vec2)*180/Math.PI); // static method that returns the angle between two vectors			trace(vec1.dotProduct(vec2)); // returns dot product of vec1 with vec2			}				private function Vector3DExamples():void{			var vec1:Vector3D = new Vector3D(1,1,1);			var vec2:Vector3D = new Vector3D(1,1,0);						trace(vec1.length); // magnitude of vec1 			trace(vec1.lengthSquared); // magnitude squared of vec1						trace(Vector3D.angleBetween(vec1,vec2)*180/Math.PI); // static method that returns the angle between two vectors			trace(vec1.dotProduct(vec2)); // returns dot product of vec1 with vec2			trace(vec1.crossProduct(vec2)); // returns cross product of vec1 with vec2					}			}}