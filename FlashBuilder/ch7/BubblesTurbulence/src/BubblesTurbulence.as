package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;		import com.physicscodes.objects.Particle;	import com.physicscodes.objects.Arrow;	public class BubblesTurbulence extends Sprite{		public function BubblesTurbulence():void{			init();		}		private function init():void{			// create bubbles						var bubbles:Array = new Array();			var color:uint = 0x00ffff;						var numBubbles:Number = 20;			var rho:Number = 0.99;						for (var i:uint=1; i<=numBubbles; i++){								var radius:Number = Math.random()*20+5;				var V:Number = 4*Math.PI*Math.pow(radius,3)/3;				var mass:Number = rho*V;				var bubble:Ball = new Ball(radius,color,mass);							bubble.pos2D=new Vector2D(Math.random()*stage.stageWidth,Math.random()*stage.stageHeight);				bubble.velo2D=new Vector2D((Math.random()-0.5)*20,0);				addChild(bubble);				bubbles.push(bubble);			}							// create velocity arrow			var arrowSprite:Sprite = new Sprite();			addChild(arrowSprite);			// make the bubbles move			var mover:BubbleTurbulenceMover=new BubbleTurbulenceMover(bubbles,arrowSprite);					mover.startTime();		}	}}