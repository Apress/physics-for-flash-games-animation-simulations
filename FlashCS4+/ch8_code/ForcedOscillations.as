package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;	import com.physicscodes.objects.Particle;			public class ForcedOscillations extends Sprite{		public function ForcedOscillations():void{			init();		}		private function init():void{			// create an object							var object:Ball;			object = new Ball(15,0x0000cc,1);			object.pos2D = new Vector2D(100,50);			object.velo2D=new Vector2D(0,0);			addChild(object);							// create a spring			var attractor:Ball;			attractor=new Ball(2,0x000000);			attractor.pos2D=new Vector2D(275,50);			attractor.velo2D=new Vector2D(0,0);			addChild(attractor)							// make the object-spring system move			var oscillator:ForcedOscillator=new ForcedOscillator(object,attractor);			oscillator.startTime(10);		}	}}