package{	import flash.display.Sprite;	import com.physicscodes.math.Vector2D;	import com.physicscodes.objects.Ball;	import com.physicscodes.objects.Wall;	public class BouncingOffInclinedWall extends Sprite{		public function BouncingOffInclinedWall():void{			init();		}		private function init():void{			// create the wall to bounce off			 				var wall:Wall;			wall=new Wall(new Vector2D(50,200),new Vector2D(700,500));            addChild(wall);			// create ball to bounce				var ball:Ball;			ball = new Ball(15,0x0000ff,90);			ball.pos2D = new Vector2D(100,50);			addChild(ball);			// trigger bouncing			var bouncer:InclinedWallBouncer=new InclinedWallBouncer(ball,wall);			bouncer.startTime(10);		}	}}