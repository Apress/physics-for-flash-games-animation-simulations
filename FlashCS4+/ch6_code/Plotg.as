package{	import flash.display.Sprite;	import com.physicscodes.math.Graph;		public class Plotg extends Sprite{					public function Plotg(){			init();		}				private function init():void{			var graph:Graph = new Graph(0,10,0,10,50,350,450,300);						graph.drawgrid(1,0.2,2,0.4);						graph.drawaxes('r / R','g');						addChild(graph);						var rA:Array = new Array();			var gA:Array = new Array();					for (var i:uint=0; i<=100; i++){				var r:Number;				rA[i] = i*0.1;				r = rA[i];				if (r<=1){					gA[i] = 9.81*r;				}else{					gA[i] = 9.81/(r*r);				}			}			graph.plot(rA, gA, 0xff0000, false, true);		}	}}