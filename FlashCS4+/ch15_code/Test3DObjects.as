package {	import flash.display.Sprite;	import flash.geom.Vector3D;		import com.physicscodes.objects.Ball3D;	public class Test3DObjects extends Sprite{				public function Test3DObjects():void {			init();		}				private function init():void {			var ball:Ball3D = new Ball3D(50);			ball.pos = new Vector3D(100,200,500);			ball.velo = new Vector3D(50,0,100);			ball.rotationZ = 90;						ball.angVelo = new Vector3D(1,0,0);			addChild(ball);			ball.move();		}			}}