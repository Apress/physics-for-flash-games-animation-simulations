package{	import flash.display.Sprite;	public class GraphExample extends Sprite{				public function GraphExample(){			init();		}				private function init():void{			var graph:Graph = new Graph(-4,4,0,20,275,380,450,350);						graph.drawgrid(1,0.2,5,1);						graph.drawaxes('x','y');						addChild(graph);						var xvals:Array = new Array(-4,-3,-2,-1,0,1,2,3,4);			var yvals:Array = new Array(16,9,4,1,0,1,4,9,16);			graph.plot(xvals, yvals, 0x0000ff);						var xA:Array = new Array();			var yA:Array = new Array();			for (var i:uint=0; i<=100; i++){				xA[i] = (i-50)*0.08;				yA[i] = xA[i]*xA[i];						   			}			graph.plot(xA, yA, 0xff0000, false, true);		}	}}