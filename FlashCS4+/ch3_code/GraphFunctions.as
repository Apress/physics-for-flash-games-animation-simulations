package{	import flash.display.Sprite;		public class GraphFunctions extends Sprite{		public function GraphFunctions(){			init();		}		private function init():void{//			var graph:Graph = new Graph(-4,4,-10,10,275,210,450,350);//			var graph:Graph = new Graph(-2.5,2.5,-10,10,275,210,450,350);			//			var graph:Graph = new Graph(-3,3,0,20,275,380,480,350);			var graph:Graph = new Graph(-3,3,0,1,275,380,480,350);						graph.drawgrid(1,0.2,0.2,0.05);	// 			graph.drawgrid(1,0.2,5,1);	//			graph.drawgrid(0.5,0.1,2,0.5);							graph.drawaxes('x','y');			addChild(graph);			var xA:Array = new Array();			var yA:Array = new Array();			var y1A:Array = new Array();			for (var i:uint=0; i<=100; i++){			//	xA[i] = (i-50)*0.08;				xA[i] = (i-50)*0.06;						  	//	xA[i] = (i-50)*0.05;					yA[i] = f(xA[i]);						 								y1A[i] = f(-xA[i]);							}			graph.plot(xA,yA,0xff0000,false,true);			graph.plot(xA,y1A,0x0000ff,false,true);					}		private function f(x:Number):Number{			var y:Number;			y = Math.exp(-x*x);			//			y = Math.exp(x);			//y = -0.5*Math.pow(x,5) + 3*Math.pow(x,3) + x*x - 2*x - 3;			//y = x*x*x + x*x - 2*x - 3;			//y = x*x - 2*x - 3;			//y = 2*x + 1;			return y;		}	}}