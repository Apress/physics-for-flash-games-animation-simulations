package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;		import com.physicscodes.objects.Particle;	public class Springs extends Sprite{		public function Springs():void{			init();		}		private function init():void{			// create a stationary center			var center:Ball;			center = new Ball(2,0x000000);			center.pos2D = new Vector2D(400,150);						addChild(center);			// create a moving particle						var particle:Ball;			particle = new Ball(5,0x0000ff);			particle.pos2D = new Vector2D(200,150);						addChild(particle);			// create endpoint markers			var end1:Ball;			end1 = new Ball(5,0xff0000);			end1.pos2D = new Vector2D(200,150);						addChild(end1);						var end2:Ball;			end2 = new Ball(5,0xff0000);			end2.pos2D = new Vector2D(600,150);						addChild(end2);			// make the particle orbit the sun			var oscillator:Oscillator=new Oscillator(particle,center);					//oscillator.startTime(40,1000);			oscillator.startTime(50);		}	}}