package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;		public class Projectile extends Sprite{		public function Projectile():void{			init();		}		private function init():void{			var particle:Ball = new Ball(10,0x0000ff,1);			particle.pos2D = new Vector2D(50,550);			particle.velo2D = new Vector2D(30,-100);						addChild(particle);			var mover:Projector=new Projector(particle);					mover.startTime(50);		}	}}