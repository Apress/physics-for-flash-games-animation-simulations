package{	import flash.display.Sprite;	import com.physicscodes.math.Vector2D;		import com.physicscodes.objects.Particle;	import com.physicscodes.objects.Star;		public class MultiGravity extends Sprite{		public function MultiGravity():void{			init();		}		private function init():void{  			// create a stationary attracting nucleus			//var massNucleus:Number = 1;							var massNucleus:Number = 1000;					var nucleus:Star = new Star(20,0x333333,massNucleus);			nucleus.pos2D = new Vector2D(stage.stageWidth/2,stage.stageHeight/2);									addChild(nucleus);			// initial distribution of stars 			var stars:Array=new Array();			var numStars:uint = 200;			var massStar:Number = 100;						var vmag:Number = 10;						for (var i:Number=0;i<numStars; i++){				var star:Star = new Star(2,0xffff00,massStar*(Math.random()+0.1));				star.pos2D = new Vector2D(Math.random()*stage.stageWidth,Math.random()*stage.stageHeight);				star.velo2D = new Vector2D(vmag*(Math.random()-0.5),vmag*(Math.random()-0.5));				addChild(star);				stars.push(star);			}						// make the stars move			var mover:MultiGravitator=new MultiGravitator(stars,nucleus);					mover.startTime();		}	}}