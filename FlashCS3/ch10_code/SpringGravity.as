package{	import flash.display.Sprite;	import com.physicscodes.math.Vector2D;		import com.physicscodes.objects.Ball;	public class SpringGravity extends Sprite{		public function SpringGravity():void{			init();		}		private function init():void{			var numParticles:Number=100;			var vmax:Number=100;			var particles:Array = new Array();			for (var i:uint=0; i<numParticles; i++){				var particle:Ball;				particle = new Ball(4,0x000000,1);					particle.pos2D = new Vector2D(Math.random()*stage.stageWidth,Math.random()*stage.stageHeight);				particle.velo2D = new Vector2D((Math.random()-0.5)*vmax,(Math.random()-0.5)*vmax);				addChild(particle);				particles.push(particle);			}						// spring force kr			var center:Ball;			center = new Ball(20,0xff0000,1,1);			center.pos2D = new Vector2D(400,300);			addChild(center);				var timeStep:Number = 10;			var forcer:SpringGravityForcer=new SpringGravityForcer(particles,center);					forcer.startTime(timeStep);		}	}}