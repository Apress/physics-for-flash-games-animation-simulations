package{	import flash.display.Shape;	import flash.display.Sprite;	import flash.events.Event;		public class Wall extends Sprite{					private var _shape:Shape;		private var _rWidth:Number;		private var _rHeight:Number;		private var _rColor:Number;		private var _rAlpha:Number;				public function Wall(pWidth:Number=400,pHeight:Number=400,pColor:uint=0x0000ff,pAlpha:Number=0.5){			_rWidth = pWidth;			_rHeight = pHeight;			_rColor = pColor;			_rAlpha = pAlpha;			init();		}				private function init():void{			_shape = new Shape();			_shape.graphics.beginFill(_rColor,_rAlpha);			_shape.graphics.drawRect(-_rWidth/2, -_rHeight/2, _rWidth, _rHeight);			addChild(_shape);		}				}		}