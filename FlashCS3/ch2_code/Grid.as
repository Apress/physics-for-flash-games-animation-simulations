package{	import flash.display.Sprite;		public class Grid extends Sprite{				private var xMin:Number=50;		private var xMax:Number=400;		private var yMin:Number=50;		private var yMax:Number=300;		private var xStep:Number=10;		private var yStep:Number=10;				public function Grid(){			init();		}				private function init():void{			graphics.clear();			graphics.lineStyle(1,0x000000,1);			var imax:Number = Math.floor((xMax-xMin)/xStep);			for (var i:int=0; i<=imax; i++){				graphics.moveTo(xMin+xStep*i,yMin);				graphics.lineTo(xMin+xStep*i,yMax);			}			var jmax:Number = Math.floor((yMax-yMin)/yStep);			for (var j:int=0; j<=jmax; j++){				graphics.moveTo(xMin,yMin+yStep*j);				graphics.lineTo(xMax,yMin+yStep*j);			}			graphics.lineStyle(1,0x000000,0);			graphics.beginFill(0x00ff00);			graphics.drawRect(10, 10, 20, 30);			graphics.endFill();		}			}	}