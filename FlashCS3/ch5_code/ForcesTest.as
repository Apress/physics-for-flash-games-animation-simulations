package{	import flash.display.Sprite;	import com.physicscodes.objects.Ball;	import com.physicscodes.math.Vector2D;			public class ForcesTest extends Sprite{		public function ForcesTest():void{			init();		}		private function init():void{			var pos0:Vector2D = new Vector2D(50,400);			var velo0:Vector2D = new Vector2D(60,-60);						var ball:Ball;			ball = new Ball(15,0x0000ff);			ball.pos2D = pos0;			ball.velo2D = velo0;			addChild(ball);			var forcer:TestForcer=new TestForcer(ball);					forcer.startTime();		}	}}